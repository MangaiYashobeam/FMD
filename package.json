{
  "name": "dealersface",
  "version": "1.0.0",
  "description": "Dealers Face - Auto Dealer Facebook Marketplace Automation Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc && tsc-alias && npm run build:web",
    "build:web": "cd web && npm ci && npm run build",
    "start": "node dist/server.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seeds/subscriptionPlans.seed.ts",
    "db:seed:admin": "tsx prisma/seeds/rootAdmin.seed.ts",
    "db:seed:all": "tsx prisma/seeds/subscriptionPlans.seed.ts && tsx prisma/seeds/rootAdmin.seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "facebook",
    "marketplace",
    "automation",
    "auto-dealer",
    "dealersface"
  ],
  "author": "Dealers Face",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-ses": "^3.970.0",
    "@prisma/client": "^5.20.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.5",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "basic-ftp": "^5.0.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.15.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "pdfkit": "^0.17.2",
    "prisma": "^5.20.0",
    "stripe": "^17.7.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "winston": "^3.15.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/geoip-lite": "^1.4.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:lru-cache@^10.0.0",
    "tar": "^7.5.3",
    "diff": "^8.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
